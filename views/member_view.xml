<odoo>
    
        <record id="view_library_member_list" model="ir.ui.view">
            <field name="name">acs.member.list</field>
            <field name="model">acs.member</field>
            <field name="arch" type="xml">
                <list string="Library Members">
                    <field name="member_id"/>
                    <field name="name"/>
                    
                </list>
            </field>
        </record>

        <record id="view_library_member_form" model="ir.ui.view">
            <field name="name">acs.member.form</field>
            <field name="model">acs.member</field>
            <field name="arch" type="xml">
                <form string="Library Member">
                    <sheet>
                        <group>
                            <field name="member_id"/>
                            <field name="name"/>
                            <field name="dob"/>
                            <field name="emailid"/>
                            <field name="contactno"/>
                            <field name="subscription_fees"/>
                            <field name="subscription_start_date"/>
                            <field name="subscription_end_date"/>
                            <field name="book_ids" widget="many2many_tags"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_library_member" model="ir.actions.act_window">
            <field name="name">Library Members</field>
            <field name="res_model">acs.member</field>
            <field name="view_mode">list,form</field>
            <field name="view_id" ref="view_library_member_list"/>
        </record>

       
</odoo>

